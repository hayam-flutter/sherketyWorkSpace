{
    "name": "sherkety_flutter_app",
    "root": "apps/sherkety_flutter_app",
    "targets": {
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "cd apps/sherkety_flutter_app && flutter run -d emulator-5554"
            }
        },
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "cd apps/sherkety_flutter_app && flutter build apk"
            }
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "cd apps/sherkety_flutter_app && flutter test"
            }
        }
    }
}